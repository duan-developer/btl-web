<% if (error && error.length) { %>
  <div class="alert alert-error">
    <% error.forEach(function (message) { %>
      <p><%= message %></p>
    <% }); %>
  </div>
<% } %>

<% if (success && success.length) { %>
  <div class="alert alert-success">
    <% success.forEach(function (message) { %>
      <p><%= message %></p>
    <% }); %>
  </div>
<% } %>
